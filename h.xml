<script>
    var API_HOST = "https://sheets.googleapis.com/v4/spreadsheets/";
    //@TODO change key
    var API_KEY = "AIzaSyDIs2fHGHxFLWE-O5hLdBtxhZdfx1Jjld4";
    var SHEET_ID = "1JDo6BzfC0StJKLkYLl_g1AxHrgBWQZbjNddBI9o1N78";
    var RAW_DATA_GIR = "1896516446";

    var CACHE = function () {
        var today = new Date();
        var cacheKey = 'cache_' + today.getMonth() + '_' + today.getDate() + window.location.href;

        var lsCache =  localStorage && localStorage.getItem(cacheKey);
        lsCache = lsCache && JSON.parse(lsCache);

        var bag = {
            organization: {},
            region: {},
            state: {},
        };
        bag = lsCache || bag;

        return {
            getData: function (resource) {
                if (resource in bag) {
                    return bag[resource];
                }
                return false;
            },
            addData: function (resource, data) {
                bag[resource] = data;
                localStorage && localStorage.setItem(cacheKey, JSON.stringify(bag));
            }
        };
    }();

    var SHEET_DATA_SOURCE = {
        organization: API_HOST + SHEET_ID + "/values/RawData!E3:E?" + $.param({
            majorDimension: 'COLUMNS',
            key: API_KEY
        }),
        region: API_HOST + SHEET_ID + "/values/RawData!F3:F?" + $.param({
            majorDimension: 'COLUMNS',
            key: API_KEY
        }),
        state: API_HOST + SHEET_ID + "/values/RawData!AX3:AX?" + $.param({
            majorDimension: 'COLUMNS',
            key: API_KEY
        }),
    };

    var SheetApi = function (sheetId, apiKey) {
        var baseUrl = 'https://sheets.googleapis.com/v4/spreadsheets/' + sheetId;

        var getQueryParameters = function () {
            return {
                key: apiKey
            };
        };

        function getSheet(sheet) {
            return $.get(baseUrl + "/values/" + sheet, getQueryParameters());
        }

        return {
            listSheets: function () {
                return $.get(baseUrl, getQueryParameters());
            },
            getSheet1: function (range) {
                return getSheet('Sheet1' + (range ? ('!' + range) : ''));
            }
        };
    }(SHEET_ID, API_KEY);

    $(function () {
        var filterByTerm = function (data, term) {
            return data.filter(function (item) {
                return item.toUpperCase().indexOf(term.toUpperCase()) >= 0;
            });
        };

        $.map(['organization', 'region', 'state'], function (fieldName) {
            $("#" + fieldName).autocomplete({
                minLength: 1,
                source: function (request, response) {
                    var cache = CACHE.getData(fieldName);
                    var term = request.term;
                    if (term in cache) {
                        response(cache[ term ]);
                        CACHE.addData(fieldName, cache);
                        return;
                    }

                    var url = SHEET_DATA_SOURCE[fieldName];

                    var data = CACHE.getData(url);
                    if (data) {
                        cache[term] = filterByTerm(data, term);
                        cache[term] = cache[term].length ? cache[term] : ['No matches'];
                        CACHE.addData(fieldName, cache);
                        response(cache[term]);
                        return;
                    }
                    $.getJSON(url, {}, function (data, status, xhr) {
                        data = data.values[0].filter(function (item, pos, self) {
                            return self.indexOf(item) == pos;
                        }).sort();

                        CACHE.addData(url, data);

                        cache[term] = filterByTerm(data, term);
                        cache[term] = cache[term].length ? cache[term] : ['No matches'];
                        CACHE.addData(fieldName, cache);
                        response(cache[term]);
                    });
                }
            });
        });

        $('input[type="checkbox"]').checkboxradio({
            icon: true
        });
    });

    function search() {
        var searchButton = $('#button-search');
        searchButton.attr('disabled', true);
        loading();

        var container = $('#search-body table tbody');
        container.html("").hide();

        var criterias = [];
        $.map($('#search-form input'), function (el) {
            switch (el.type) {
                case 'text':
                    el.value && (criterias.push(el.name + ' = "' + el.value + '"'));
                    break;
                case 'checkbox':
                    el.checked && (criterias.push(el.name + ' = "' + el.value + '"'));
                    break;
            }
        });
        var query = "select * " + (criterias.length ? "where " + criterias.join(' and ') : '');

        $.ajax('https://spreadsheets.google.com/tq', {
            data :{
                tqx: 'out:html',
                key: SHEET_ID,
                gid: RAW_DATA_GIR,
                tq: query
            },
            crossDomain: true,
            dataType: 'html',
            type:'GET'
        }).done(function (html){
            $('#search-body table tbody').html($(html).find('table tbody').html());
//        SheetApi.getSheet1("A1:AA").done(function (data) {
//            var header = data.values.shift();
//            var body = data.values.reduce(function (body, item) {
//                if (header.indexOf('profile_picture') >= 0) {
//                    var ix = header.indexOf('profile_picture');
//                    item[ix] = '<img src="' + item[ix] + '" class="img-thumbnail">';
//                }
//                return body
//                    + "<tr><td>" + item.join('</td><td>') + "</td></tr>";
//            }, "");
//
//            container.html(
//                "<tr><th>" + header.join('</th><th>') + "</th></tr>"
//                + body
//                );
//            var searchBody = $('#search-body table');
//            $.map($('#field-list input:checked'), function (el) {
//                var th = searchBody.find('th:contains(' + el.name + ')')[0];
//
//                searchBody.find('tr :nth-child(' + (th.cellIndex + 1) + ')').hide()
//            });
//
//            container.show();
        }).fail(function () {

        }).always(function () {
            searchButton.attr('disabled', false);
            $("#progressbar").hide();
        });

    }

    function reset() {
        $.map($('#search-form input'), function (el) {
            switch (el.type) {
                case 'text':
                    el.value = '';
                    break;
                case 'checkbox':
                    el.checked && $(el).click();
                    break;
            }
        });
    }

    function loading() {

        $(function () {
            var progressbar = $("#progressbar"),
                progressLabel = $(".progress-label");

            progressbar.show();

            progressbar.progressbar({
                value: false,
                change: function () {
                    progressLabel.text(progressbar.progressbar("value") + "%");
                },
                complete: function () {
                    progressLabel.text("Complete!");
                }
            });

            function progress() {
                var val = progressbar.progressbar("value") || 0;

                progressbar.progressbar("value", val + 2);

                if (val < 40) {
                    setTimeout(progress, 40);
                } else if (val < 60) {
                    setTimeout(progress, 300);
                } else {
                    setTimeout(progress, 600);
                }
            }

            setTimeout(progress, 3000);
        });
    }
</script>

